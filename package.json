{
  "name": "log-parser-app",
  "version": "1.0.0",
  "description": "Node.js CLI-driven application that parses log files and outputs filtered error logs.",
  "main": "parser.js",
  "scripts": {
    "build": "npx tsc",
    "start:dev": "ts-node src/parser.ts --input ./app.log --output ./errors.json",
    "dev": "ts-node src/dev.ts --input ./app.log --output ./errors.json",
    "test": "jest",
    "test:dev": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahsan-babar/log-parser-app.git"
  },
  "keywords": [
    "logs",
    "parser",
    "file",
    "error",
    "level",
    "cli",
    "messages"
  ],
  "author": "Ahsan Babar <ahsanbabar147@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ahsan-babar/log-parser-app/issues"
  },
  "homepage": "https://github.com/ahsan-babar/log-parser-app#readme",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.5",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^4.7.4"
  }
}